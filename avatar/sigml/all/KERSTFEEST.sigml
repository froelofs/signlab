    </sign_nonmanual>
    <sign_manual>
      <sign_manual both_hands="true" lr_symm="true">
        <handconfig handshape="flat" thumbpos="across"/>
        <handconfig extfidir="o" second_extfidir="uo"/>
        <handconfig palmor="dl"/>
        <handconstellation contact="touch">
          <location_hand location="thumbside"/>
          <location_hand location="thumbside"/>
          <location_bodyarm contact="close" location="head"/>
        </handconstellation>
        <par_motion>
          <directedmotion curve="d" curve_size="small" direction="dr" fast="true" size="small"/>
          <tgt_motion>
            <changeposture/>
            <handconfig extfidir="o" palmor="d"/>
          </tgt_motion>
        </par_motion>
        <tgt_motion>
          <par_motion>
            <directedmotion curve="u" curve_size="small" direction="ul" fast="true" size="small"/>
            <tgt_motion>
              <changeposture/>
              <handconfig extfidir="o" palmor="dl" second_extfidir="uo"/>
            </tgt_motion>
          </par_motion>
          <location_bodyarm contact="close" location="head" second_location="chin"/>
        </tgt_motion>
        <par_motion>
          <directedmotion curve="d" curve_size="small" direction="dr" fast="true" size="small"/>
          <tgt_motion>
            <changeposture/>
            <handconfig extfidir="o" palmor="d"/>
          </tgt_motion>
        </par_motion>
      </sign_manual>
      <sign_manual both_hands="true" lr_symm="true">
        <handconfig handshape="finger2" specialfingers="5" thumbpos="out"/>
        <handconfig extfidir="u" second_extfidir="ur"/>
        <handconfig palmor="l"/>
