        <mouth_picture picture="onA"/>
      </mouthing_tier>
    </sign_nonmanual>
    <sign_manual>
      <handconfig handshape="flat" thumbpos="opposed"/>
      <handconfig extfidir="uo"/>
      <handconfig palmor="l"/>
      <location_bodyarm contact="close" location="forehead" second_location="forehead" side="right_at">
        <location_hand digits="1" location="midjoint" side="dorsal"/>
      </location_bodyarm>
      <tgt_motion>
