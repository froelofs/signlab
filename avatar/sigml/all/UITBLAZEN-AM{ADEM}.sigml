      </mouthing_tier>
    </sign_nonmanual>
    <sign_manual>
      <handconfig handshape="pinchall"/>
      <handconfig extfidir="ul"/>
      <handconfig palmor="dr" second_palmor="r"/>
      <location_bodyarm contact="touch" location="lips">
        <location_hand digits="3" location="base" side="dorsal"/>
      </location_bodyarm>
      <par_motion>
        <directedmotion direction="do" size="big"/>
