        <directedmotion curve="l" curve_size="small" direction="u" fast="true"/>
        <tgt_motion fast="true">
          <changeposture/>
          <handconfig extfidir="u" palmor="u"/>
        </tgt_motion>
      </par_motion>
    </sign_manual>
  </hamgestural_sign>
</sigml>
