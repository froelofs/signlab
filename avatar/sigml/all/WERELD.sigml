            <handconfig extfidir="u" palmor="u" second_extfidir="uo" second_palmor="ul"/>
          </tgt_motion>
        </par_motion>
        <handconstellation contact="touch">
          <location_hand digits="5" location="tip"/>
          <location_hand digits="5" location="tip"/>
        </handconstellation>
