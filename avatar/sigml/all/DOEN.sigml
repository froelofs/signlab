      <tgt_motion>
        <split_motion>
          <directedmotion direction="d" size="small"/>
          <nomotion/>
        </split_motion>
        <handconstellation contact="touch"/>
      </tgt_motion>
    </sign_manual>
  </hamgestural_sign>
</sigml>
