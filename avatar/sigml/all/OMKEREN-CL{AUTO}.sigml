              <handconfig handshape="flat" mainbend="round" second_handshape="flat" second_mainbend="bent"/>
              <handconfig extfidir="ui" palmor="dr" second_extfidir="ul"/>
            </tgt_motion>
          </par_motion>
        </seq_motion>
      </sign_manual>
    </sign_manual>
  </hamgestural_sign>
</sigml>
