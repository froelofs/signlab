                    </location_hand>
                </split_location>
            </tgt_motion>
            <directedmotion direction="o" size="small"/>
        </sign_manual>
        <sign_nonmanual>
            <mouthing_tier>
                <mouth_picture picture="a:mstelve:" speed="1.2"/>
            </mouthing_tier>
            <body_tier>
