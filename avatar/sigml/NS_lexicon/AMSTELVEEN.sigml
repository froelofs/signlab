                    <split_handconfig>
                        <handconfig extfidir="ul" palmor="dl"/>
                        <handconfig extfidir="ur" palmor="dl"/>
                    </split_handconfig>
                </tgt_motion>
                <split_location>
                    <location_bodyarm location="shouldertop"/>
                    <location_hand contact="touch" location="midjoint" second_digits="2" second_location="nail" second_side="palmar">
