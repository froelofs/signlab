            <handconfig handshape="finger2" thumbpos="across"/>
            <split_handconfig>
                <handconfig extfidir="ul" palmor="dl"/>
                <handconfig extfidir="ur" palmor="dl"/>
            </split_handconfig>
            <split_location>
                <location_hand contact="touch" location="midjoint" second_digits="2" second_location="nail" second_side="palmar">
                    <location_hand location="midjoint" second_digits="2" second_location="nail" second_side="palmar"/>
                </location_hand>
                <location_bodyarm location="nose"/>
            </split_location>
            <par_motion>
                <directedmotion direction="r" size="big"/>
                <split_motion>
                    <wristmotion motion="swinging"/>
                    <nomotion/>
