<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE sigml SYSTEM "sigml100.dtd"><sigml>  <!--######## SPITZ1A ########-->  <sigmlsign>    <sign_manual>      <split_handconfig>        <handconfig handshape="cee12" d2345="bent" thumbpos="across"/>        <handconfig handshape="finger2" thumbpos="across"/>      </split_handconfig>      <split_handconfig>        <handconfig extfidir="o" palmor="l"/>        <handconfig extfidir="ur" palmor="dr"/>      </split_handconfig>      <location_hand digits="2" contact="interlock">        <location_hand location="pad" digits="12"/>      </location_hand>      <par_motion>        <directedmotion direction="ur" size="small"/>        <tgt_motion>          <changeposture/>          <handconfig handshape="pinch12" d2345="bent"/>        </tgt_motion>      </par_motion>    </sign_manual>  </sigmlsign></sigml>