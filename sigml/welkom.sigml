        <handconfig handshape="finger2345" thumbpos="out"/>
        <handconfig extfidir="uo"/>
        <handconfig palmor="l"/>
        <handconstellation contact="close">
          <location_bodyarm contact="close" location="shoulders" second_location="chest"/>
        </handconstellation>
        <circularmotion axis="l" size="small"/>
      </sign_manual>
    </hamgestural_sign>
      <hamgestural_sign gloss="SIGNLAB AMSTERDAM">
          <sign_manual both_hands="true" lr_symm="true">
              <handconfig handshape="cee12"/>
              <handconfig extfidir="o" second_extfidir="or"/>
              <handconfig palmor="l"/>
              <split_location>
                  <location_bodyarm location="shoulders"/>
