      <tgt_motion>
        <directedmotion direction="d" speed="3" size="small"/>
        <handconstellation contact="close">
          <location_hand digits="2" location="midjoint" side="right_beside"/>
          <location_hand digits="2" location="midjoint" side="right_beside"/>
