  </hamgestural_sign>
  <hamgestural_sign gloss="SIGNLAB AMSTERDAM">
    <sign_nonmanual>
      <mouthing_tier>
        <mouth_picture picture="iin"/>
      </mouthing_tier>
    </sign_nonmanual>
