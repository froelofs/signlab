      <tgt_motion>
        <split_motion>
          <par_motion>
            <directedmotion curve="u" curve_size="small" direction="il" size="big"/>
            <tgt_motion>
              <changeposture/>
              <handconfig extfidir="ol"/>
            </tgt_motion>
          </par_motion>
          <par_motion>
            <directedmotion curve="u" curve_size="small" direction="il"/>
            <tgt_motion>
              <changeposture/>
              <handconfig extfidir="o" second_extfidir="or"/>
