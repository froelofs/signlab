      <handconfig handshape="finger2" mainbend="bent" second_handshape="finger2" second_thumbpos="across" thumbpos="across" />
      <split_handconfig>
        <handconfig extfidir="u" palmor="d" second_extfidir="o" second_palmor="dl" />
        <handconfig extfidir="o" palmor="r" second_extfidir="uo" second_palmor="dr" />
      </split_handconfig>
      <location_bodyarm location="shoulders" />
      <split_motion>
        <directedmotion curve="u" direction="r" size="small"  />
        <nomotion />
      </split_motion>
      <split_motion>
        <par_motion>
          <directedmotion curve="l" curve_size="big" direction="ul" size="small" />
          <tgt_motion>
            <changeposture />
            <handconfig extfidir="u" palmor="d" second_extfidir="o" />
          </tgt_motion>
        </par_motion>
        <par_motion>
          <directedmotion curve="l" curve_size="big" direction="l" size="small" />
          <tgt_motion>
            <changeposture />
            <handconfig extfidir="o" palmor="d" second_extfidir="uo" />
          </tgt_motion>
        </par_motion>
      </split_motion>
      <split_motion>
        <par_motion>
          <directedmotion curve="l" curve_size="big" direction="r" size="small" />
          <tgt_motion>
            <changeposture />
            <handconfig extfidir="u" palmor="d" second_extfidir="o" />
          </tgt_motion>
        </par_motion>
        <par_motion>
          <directedmotion curve="l" curve_size="big" direction="ur" />
          <tgt_motion>
            <changeposture />
            <handconfig extfidir="u" palmor="d" second_extfidir="uo" />
          </tgt_motion>
        </par_motion>
      </split_motion>
    </sign_manual>
    <sign_nonmanual>
      <body_tier>
        <body_movement movement="ST" />
      </body_tier>
      <facialexpr_tier />
    </sign_nonmanual>
  </hamgestural_sign>
  <hamgestural_sign gloss="ZIEK">
      <sign_nonmanual>
        <mouthing_tier>
          <mouth_picture picture="ik"/>
        </mouthing_tier>
      </sign_nonmanual>
      <sign_manual>
        <split_handconfig>
          <handconfig handshape="pinch12open" mainbend="bent"/>
          <handconfig handshape="flat" second_handshape="flat" second_mainbend="round"/>
        </split_handconfig>
        <split_handconfig>
          <handconfig extfidir="ol" palmor="d"/>
          <handconfig extfidir="or" palmor="d"/>
        </split_handconfig>
        <handconstellation contact="close">
          <location_hand location="handback" side="dorsal"/>
          <location_hand digits="2" location="tip"/>
        </handconstellation>
        <rpt_motion >
          <tgt_motion>
            <directedmotion direction="d"/>
            <handconstellation contact="touch"/>
          </tgt_motion>
        </rpt_motion>
      </sign_manual>
    </hamgestural_sign>
    <hamgestural_sign gloss="AANWEZIG">
